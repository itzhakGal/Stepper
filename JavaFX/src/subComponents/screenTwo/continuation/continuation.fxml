<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox stylesheets="@continuation.css" visible="false" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="subComponents.screenTwo.continuation.ContinuationController">
   <children>
      <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="continuationLabel" alignment="CENTER" contentDisplay="CENTER" style="-fx-font-weight: bold;" text="Flow Continuation data" textOverrun="CLIP" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
         <children>
            <ListView fx:id="flowNameContinuationListView" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="50.0" right="50.0" />
               </HBox.margin>
            </ListView>
         </children>
      </HBox>
      <HBox alignment="BOTTOM_RIGHT">
         <children>
            <Button fx:id="continueToFlowButton" mnemonicParsing="false" onAction="#continueToFlowButtonAction" text="Continue to Flow" visible="false" HBox.hgrow="ALWAYS" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
