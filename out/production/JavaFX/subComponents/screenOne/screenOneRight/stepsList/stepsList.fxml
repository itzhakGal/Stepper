<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<HBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="subComponents.screenOne.screenOneRight.stepsList.StepListController">
   <children>
      <Accordion fx:id="stepsListAccordion" HBox.hgrow="ALWAYS">
         <panes>
            <TitledPane animated="false">
              <content>
                <AnchorPane>
                     <children>
                        <fx:include fx:id="stepDetailsComponent" source="stepDetails/stepDetails.fxml" />
                     </children>
                  </AnchorPane>
              </content>
            </TitledPane>
         </panes>
         <HBox.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </Accordion>
   </children>
</HBox>
